#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/tdenisenko/code/hackaton-istanbul/wagmi/packages/cli/dist/node_modules:/Users/tdenisenko/code/hackaton-istanbul/wagmi/packages/cli/node_modules:/Users/tdenisenko/code/hackaton-istanbul/wagmi/packages/node_modules:/Users/tdenisenko/code/hackaton-istanbul/wagmi/node_modules:/Users/tdenisenko/code/hackaton-istanbul/node_modules:/Users/tdenisenko/code/node_modules:/Users/tdenisenko/node_modules:/Users/node_modules:/node_modules:/Users/tdenisenko/code/hackaton-istanbul/wagmi/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/tdenisenko/code/hackaton-istanbul/wagmi/packages/cli/dist/node_modules:/Users/tdenisenko/code/hackaton-istanbul/wagmi/packages/cli/node_modules:/Users/tdenisenko/code/hackaton-istanbul/wagmi/packages/node_modules:/Users/tdenisenko/code/hackaton-istanbul/wagmi/node_modules:/Users/tdenisenko/code/hackaton-istanbul/node_modules:/Users/tdenisenko/code/node_modules:/Users/tdenisenko/node_modules:/Users/node_modules:/node_modules:/Users/tdenisenko/code/hackaton-istanbul/wagmi/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../packages/cli/dist/cli.js" "$@"
else
  exec node  "$basedir/../../../../packages/cli/dist/cli.js" "$@"
fi
